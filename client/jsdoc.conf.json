{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "sourceType": "module",
    "plugins": ["plugins/markdown", "better-docs"],
    "templates": {
        "better-docs": {
            "name": "My React components"
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.md"
    }
}
